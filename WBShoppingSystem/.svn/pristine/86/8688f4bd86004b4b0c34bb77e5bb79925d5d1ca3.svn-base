/* TB_window */
#TB_overlay{position:fixed;z-index:100;top:0px;left:0px;height:100%;width:100%;}
#TB_window a{color:#fff;}
.TB_overlayBG{background-color:#000;filter:alpha(opacity=50);-moz-opacity:0.50;opacity:0.50;}
*html #TB_overlay{position:absolute;height:expression(document.body.scrollHeight > document.body.offsetHeight ? document.body.scrollHeight : document.body.offsetHeight + 'px');}
#TB_window{position:fixed;background:#fff;z-index:102;color:#000;display:none;border:5px solid #525252;text-align:left;top:50%;*top:12%;_top:50%;left:50%;-moz-box-shadow:3px 3px 3px rgba(0,0,0,0.2);-webkit-box-shadow:3px 3px 3px rgba(0,0,0,0.2);box-shadow:3px 3px 3px rgba(0,0,0,0.2);-moz-border-radius:3px 3px 3px 3px;-webkit-border-radius:3px 3px 3px 3px;}
*html #TB_window{position:absolute;margin-top:expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px');}
#TB_window img#TB_Image{display:block;margin:15px 0 0 15px;border-right:1px solid #ccc;border-bottom:1px solid #ccc;border-top:1px solid #666;border-left:1px solid #666;}
#TB_ajaxWindowTitle{float:left;padding:7px 0 5px 10px;margin-bottom:1px;color:#fff;font-size:14px;font-weight:800;}
#TB_closeAjaxWindow{float:right;padding:6px 10px 0 0;}
#TB_title{background-color:#990000;height:30px;}
#TB_load{position:fixed;display:none;height:13px;width:208px;z-index:103;top:50%;left:50%;margin:-6px 0 0 -104px;}
*html #TB_load{position:absolute;margin-top:expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px');}
#TB_ajaxContent{padding:10px 15px;}
/* discretion */
.dish3,.dish3-01{line-height:30px;color:#999;}
.dish3{padding:20px 0 15px 20px;}
.dish3-01{padding:25px 0 5px 20px;}
.disp{line-height:22px;color:#666;margin:0 10px;padding-bottom:25px;border-bottom:1px dotted #999;}
.disp-indent{text-indent:2em;}
.distable td,.distable th{border:1px solid #E4E4E4;padding:4px 0;line-height:20px;text-align:center;color:#999;font-family:Arial;}
.distable td.de-td1{background-color:#eee;font-weight:bold;}
.distab{background-image:url(../images/mask-bg.png);background-repeat:repeat-x;height:40px;overflow:hidden;margin:20px 0;}
.distab a{color:#ffd5d5;font-weight:400;font-size:18px;font-family:"微软雅黑","黑体";display:block;float:left;height:40px;line-height:40px;width:127px;text-align:center;}
.distab a.cur{color:#fff260;}
.distab a:hover{text-decoration:none;}