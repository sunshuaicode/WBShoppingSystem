package edu.shopsys.model;

// 收货地址实体类
public class ShippingAddress {
    // 收货地址的ID
    private String address_id;
    // 收货地址中的收件人姓名
    private String address_cnee;
    // 收件人的电话
    private String address_phone;
    // 收件人的地址
    private String address_add;
    // 收件人的邮编
    private String address_pc;
    // 此收货地址的用户ID,和User中的ID关联
    private String address_user_id;

    public ShippingAddress() {
        super();
    }

    public ShippingAddress(String address_id, String address_cnee, String address_phone,
            String address_add, String address_pc, String address_user_id) {
        super();
        this.address_id = address_id;
        this.address_cnee = address_cnee;
        this.address_phone = address_phone;
        this.address_add = address_add;
        this.address_pc = address_pc;
        this.address_user_id = address_user_id;
    }

    public String getAddress_id() {
        return address_id;
    }

    public void setAddress_id(String address_id) {
        this.address_id = address_id;
    }

    public String getAddress_cnee() {
        return address_cnee;
    }

    public void setAddress_cnee(String address_cnee) {
        this.address_cnee = address_cnee;
    }

    public String getAddress_phone() {
        return address_phone;
    }

    public void setAddress_phone(String address_phone) {
        this.address_phone = address_phone;
    }

    public String getAddress_add() {
        return address_add;
    }

    public void setAddress_add(String address_add) {
        this.address_add = address_add;
    }

    public String getAddress_pc() {
        return address_pc;
    }

    public void setAddress_pc(String address_pc) {
        this.address_pc = address_pc;
    }

    public String getAddress_user_id() {
        return address_user_id;
    }

    public void setAddress_user_id(String address_user_id) {
        this.address_user_id = address_user_id;
    }

}
